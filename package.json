{
  "name": "angular-sidebar",
  "version": "1.1.0",
  "description": "an angular sidebar component",
  "main": "./src",
  "style": "./src/angular-sidebar.css",
  "scripts": {
    "start": "npm run build-web",
    "watch": "watchify ./website/app.js -d -o ./website/built.js -v",
    "build-web": "browserify ./website/built.js | uglifyjs > ./website/built.min.js"
  },
  "keywords": [
    "angular",
    "sidebar"
  ],
  "author": "Jack Hanford <jackhanford@gmail.com> (http://jackhanford.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/hanford/angular-sidebar.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hanford/angular-sidebar/issues"
  },
  "peerDependencies": {
    "angular": ">=1.3 <2"
  },
  "files": [
    "src/"
  ],
  "dependencies": {},
  "devDependencies": {
    "angular": "~1.5.5",
    "browserify": "~13.0.1",
    "uglify": "~0.1.5",
    "watchify": "~3.7.0"
  }
}
